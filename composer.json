{
    "name": "dynamikaweb/yii2-tiny-slider",
    "description": "Vanilla slider inspired by Owl Carousel",
    "type": "yii2-extension",
    "keywords": [
        "dynamika",
        "yii",
        "yii2",
        "slider",
        "owl"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Rodrigo Dornelles",
            "email": "rodrigo@dynamika.com.br",
            "role": "Developer"
        },
        {
            "name": "Robson Soares",
            "email": "robson@dynamika.com.br",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "http://github.com/dynamikaweb/yii2-tiny-slider/issues",
        "source": "http://github.com/dynamikaweb/yii2-tiny-slider"
    },
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "*",
        "npm-asset/tiny-slider": "*" 
    },
    "replace": {
        "kv4nt/yii2-owl-carousel2": "deprecated assets, update to new dependencies",
        "dominus77/yii2-owl-carousel2": "deprecated assets, update to new dependencies"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "dynamikaweb\\tiny\\": "src"
        }
    }
}
